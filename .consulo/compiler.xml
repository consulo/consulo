<project>
  <component name="CompilerManager" url="file://$PROJECT_DIR$/out">
    <module name="consulo-group" exclude="false">
      <output url="file://$PROJECT_DIR$/target/classes" type="PRODUCTION" />
      <output url="file://$PROJECT_DIR$/target/classes" type="PRODUCTION_RESOURCE" />
      <output url="file://$PROJECT_DIR$/target/test-classes" type="TEST" />
      <output url="file://$PROJECT_DIR$/target/test-classes" type="TEST_RESOURCE" />
    </module>
    <module name="annotations" exclude="false">
      <output url="file://$PROJECT_DIR$/modules/annotations/target/classes" type="PRODUCTION" />
      <output url="file://$PROJECT_DIR$/modules/annotations/target/classes" type="PRODUCTION_RESOURCE" />
      <output url="file://$PROJECT_DIR$/modules/annotations/target/test-classes" type="TEST" />
      <output url="file://$PROJECT_DIR$/modules/annotations/target/test-classes" type="TEST_RESOURCE" />
    </module>
    <module name="util-rt" exclude="false">
      <output url="file://$PROJECT_DIR$/modules/util-rt/target/classes" type="PRODUCTION" />
      <output url="file://$PROJECT_DIR$/modules/util-rt/target/classes" type="PRODUCTION_RESOURCE" />
      <output url="file://$PROJECT_DIR$/modules/util-rt/target/test-classes" type="TEST" />
      <output url="file://$PROJECT_DIR$/modules/util-rt/target/test-classes" type="TEST_RESOURCE" />
    </module>
  </component>
  <component name="JavaCompilerConfiguration" compiler="JavacCompiler">
    <annotation-processing>
      <profile default="true" name="Default" enabled="false">
        <processorPath useClasspath="true" />
      </profile>
      <profile default="false" name="Annotation profile for annotations" enabled="true">
        <sourceOutputDir name="target\generated-sources\annotations" />
        <sourceTestOutputDir name="target\generated-test-sources\test-annotations" />
        <outputRelativeToContentRoot value="true" />
        <processorPath useClasspath="true" />
        <module name="annotations" />
      </profile>
      <profile default="false" name="Annotation profile for util-rt" enabled="true">
        <sourceOutputDir name="target\generated-sources\annotations" />
        <sourceTestOutputDir name="target\generated-test-sources\test-annotations" />
        <outputRelativeToContentRoot value="true" />
        <processorPath useClasspath="true" />
        <module name="util-rt" />
      </profile>
    </annotation-processing>
  </component>
</project>