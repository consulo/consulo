    <artifactId>consulo-sandbox-web</artifactId>
    <version>3-SNAPSHOT</version>
    <packaging>pom</packaging>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.build.cache.enabled>false</maven.build.cache.enabled>
    </properties>

    <repositories>
        <repository>
            <id>Vaadin Directory</id>
            <url>https://maven.vaadin.com/vaadin-addons</url>
        </repository>
    </repositories>

    <build>
        <plugins>
            <plugin>
                <artifactId>maven-assembly-plugin</artifactId>
                <version>${maven.assembly.plugin}</version>
                <executions>
                    <execution>
                        <id>create-sandbox</id>
                        <configuration>
                            <descriptors>
                                <descriptor>src/assembly/sandbox.xml</descriptor>
                            </descriptors>
                            <attach>false</attach>
                        </configuration>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>consulo.maven</groupId>
                <artifactId>maven-consulo-plugin</artifactId>
                <extensions>true</extensions>
                <executions>
                    <execution>
                        <phase>package</phase>
                        <goals>
                            <goal>build-index</goal>
                        </goals>
                        <configuration>
                            <pluginRoots>
                                <pluginRoot>
                                    ${project.build.directory}/consulo-sandbox-web-${project.version}-main/Consulo/platform/buildSNAPSHOT/modules/consulo
                                </pluginRoot>
                                <pluginRoot>
                                    ${project.build.directory}/consulo-sandbox-web-${project.version}-main/Consulo/platform/buildSNAPSHOT/modules/consulo.web
                                </pluginRoot>
                                <pluginRoot>
                                    ${project.build.directory}/consulo-sandbox-web-${project.version}-main/Consulo/platform/buildSNAPSHOT/plugins/consulo.sandbox.plugin
                                </pluginRoot>
                            </pluginRoots>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

